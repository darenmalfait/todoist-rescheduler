{
  "include": [
    "**/*.ts"
  ],
  "compilerOptions": {
    "lib": [
      "ES2021",
      "DOM",
      "DOM.Iterable"
    ],
    "baseUrl": ".",
    "incremental": true,
    "isolatedModules": false,
    "importsNotUsedAsValues": "error",
    "noEmit": true,
    "allowJs": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "es2020",
    "target": "es2020",
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noUncheckedIndexedAccess": true
  }
}